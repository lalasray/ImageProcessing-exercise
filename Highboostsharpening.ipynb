    "import cv2\n",
    " \n",
    "# Load the image\n",
    "image = cv2.imread(\"girl.JPEG\")\n",
    "# Blur the image\n",
    "gauss = cv2.GaussianBlur(image, (7,7), 0)\n",
    "# Apply Unsharp masking\n",
    "unsharp_image = cv2.addWeighted(image, 2, gauss, -1, 0)"
